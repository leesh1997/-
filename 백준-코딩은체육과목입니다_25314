import java.util.Scanner;

public class 코딩은체육과목입니다_25314 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int N = sc.nextInt();
		sc.close();
		String ln = "long ";
		if (N == 4) {
			System.out.println(ln + "int");
		} else {
			for (int i = 1; i <= N / 4; i++) {
				System.out.print(ln);
			}
			System.out.println("int");
		}

	}

}
