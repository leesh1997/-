class Solution {
    public int solution(int[] array, int n) {
        int answer = 0;
        
        for(int i = 0; i < array.length; i++){
            if(array[i] == n){
                answer++;
            }
        }
        
        return answer;
    }
}

// 다른 사람 풀이(정규식)

class Solution {
    public int solution(int[] array, int n) {
        int answer = 0;

        for(int num: array){
            answer += num==n? 1:0;
        }

        return answer;
    }
}
